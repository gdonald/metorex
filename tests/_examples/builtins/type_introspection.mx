x = 42
puts "type(42) = #{x.class}"

y = 3.14
puts "type(3.14) = #{y.class}"

name = "Alice"
puts "type('Alice') = #{name.class}"

is_active = true
puts "type(True) = #{is_active.class}"

numbers = [1, 2, 3]
puts "type([1, 2, 3]) = #{numbers.class}"

scores = {"Alice" => 90, "Bob" => 85}
puts "type({'Alice': 90, 'Bob': 85}) = #{scores.class}"

result = 10 + 20
puts "type(10 + 20) = #{result.class}"

puts "type(type(42)) = #{x.class.class}"
